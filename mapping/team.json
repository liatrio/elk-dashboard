{
  "index_patterns": ["team-v*"],
  "settings": {
  },
  "mappings": {
    "doc": {
      "dynamic": false,
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "@version": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "name": {
          "type": "keyword"
        },
        "users": {
          "type": "nested",
          "properties": {
            "id": {
              "type": "keyword"
            },
            "emails": {
              "type": "nested",
              "properties": {
                "email": {
                  "type": "keyword"
                }
              }
            }
          }
        },
        "projects": {
          "type": "nested",
          "properties": {
            "id": {
              "type": "keyword"
            },
            "repos": {
              "type": "nested",
              "properties": {
                "id": {
                  "type": "keyword"
                },
                "url": {
                  "type": "keyword"
                }
              }
            }
          }
        }
      }
    }
  }
}
